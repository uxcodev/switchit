{"ast":null,"code":"import ModalWindow from '@/components/ui/ModalWindow.vue';\nimport Multiselect from '@vueform/multiselect';\nexport default {\n  components: {\n    ModalWindow,\n    Multiselect\n  },\n  data() {\n    return {\n      modalComponent: null,\n      screen: 'UserTable',\n      form: {\n        firstName: \"\",\n        lastName: \"\",\n        email: this.$auth0.user.email,\n        company: \"\",\n        website: \"\",\n        countries: [],\n        markets: []\n      },\n      country_list: [\"Denmark\", \"Germany\", \"Norway\", \"Sweden\", \"United States\"]\n    };\n  },\n  computed: {\n    user() {\n      return this.$store.getters.user;\n    }\n  },\n  methods: {\n    closeModal() {\n      this.modalComponent = null;\n    },\n    openModal(component) {\n      this.modalComponent = component;\n    },\n    changePage(page) {\n      this.screen = page;\n    }\n  },\n  async mounted() {\n    // setTimeout(() => {\n    //  this.user = this.$store.getters.user\n    // }, 1000)\n  }\n};","map":{"version":3,"mappings":"AA0FA,OAAOA,WAAU,MAAO,iCAAiC;AACzD,OAAOC,WAAU,MAAO,sBAAqB;AAE7C,eAAe;EACbC,UAAU,EAAE;IACVF,WAAW;IACXC;EACF,CAAC;EACDE,IAAI,GAAG;IAEL,OAAO;MACLC,cAAc,EAAE,IAAI;MACpBC,MAAM,EAAE,WAAW;MACnBC,IAAI,EAAE;QACJC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,IAAI,CAACC,MAAM,CAACC,IAAI,CAACF,KAAK;QAC7BG,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE,EAAE;QACXC,SAAS,EAAE,EAAE;QACbC,OAAO,EAAE;MACX,CAAC;MACDC,YAAY,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,QAAQ,EAAE,QAAQ,EAAE,eAAe;IAC1E;EACF,CAAC;EACDC,QAAQ,EAAE;IACRN,IAAI,GAAG;MACL,OAAO,IAAI,CAACO,MAAM,CAACC,OAAO,CAACR,IAAG;IAChC;EACF,CAAC;EACDS,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,IAAI,CAACjB,cAAa,GAAI,IAAG;IAC3B,CAAC;IACDkB,SAAS,CAACC,SAAS,EAAE;MACnB,IAAI,CAACnB,cAAa,GAAImB,SAAQ;IAChC,CAAC;IACDC,UAAU,CAACC,IAAI,EAAE;MACf,IAAI,CAACpB,MAAK,GAAIoB,IAAG;IACnB;EACF,CAAC;EACD,MAAMC,OAAO,GAAG;IACd;IACA;IACA;EAAA;AAEJ","names":["ModalWindow","Multiselect","components","data","modalComponent","screen","form","firstName","lastName",