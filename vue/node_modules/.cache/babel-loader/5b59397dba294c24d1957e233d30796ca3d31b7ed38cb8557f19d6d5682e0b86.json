lf: flex-end\n\n  &-hidden\n    transform: translateX(300px)\n    opacity: 0\n    visibility: hidden\n\n  &-content\n    display: flex\n    flex-direction: column\n    flex-basis: auto\n    flex-wrap: nowrap\n    height: 100%\n    min-height: 100%\n    padding: 30px\n\n    ul\n      li, a, div\n        color: inherit\n        margin: 8px 0\n        list-style: none\n        text-decoration: none\n        cursor: pointer\n\n      .active\n        color: #03D0BF\n\n    select\n      color: inherit\n      font-size: inherit\n      font-weight: inherit\n      text-transform: uppercase\n      padding: 2px 4px\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}