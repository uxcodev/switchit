{"ast":null,"code":"import MainNav from \"@/components/layout/MainNav.vue\";\nimport api from \"@/api/switchit\";\nimport { useAuth0 } from \"@auth0/auth0-vue\";\nconst {\n  isAuthenticated\n} = useAuth0();\nexport default {\n  components: {\n    MainNav\n  },\n  name: \"App\",\n  data() {\n    return {\n      loggedIn: () => {\n        this.$auth0.user;\n      }\n    };\n  },\n  methods: {},\n  computed: {\n    user() {\n      return this.$auth0.user;\n    },\n    isAuthenticated() {\n      return this.$auth0.isAuthenticated;\n    },\n    isLoading() {\n      return this.$auth0.isLoading;\n    }\n  },\n  watch: {\n    async isAuthenticated(auth) {\n      console.log('user');\n      if (auth) {\n        let token = await api.createToken(this.$auth0.user.email);\n        console.log(`token: ${token}`);\n      }\n    }\n  },\n  async mounted() {\n    console.log('isAuthenticated');\n    console.log(isAuthenticated);\n    await this.$auth0.isAuthenticated;\n    console.log(this.$auth0.user.email);\n    // await api.createToken(this.$auth0.user.email)\n  }\n};","map":{"version":3,"mappings":"AAYA,OAAOA,OAAM,MAAO,iCAAiC;AACrD,OAAOC,GAAE,MAAO,gBAAgB;AAChC,SAASC,QAAO,QAAS,kBAAkB;AAC3C,MAAM;EAAEC;AAAgB,IAAID,QAAQ,EAAE;AAEtC,eAAe;EACbE,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,IAAI,EAAE,KAAK;EACXC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,MAAM;QAAE,IAAI,CAACC,MAAM,CAACC,IAAG;MAAE;IACrC,CAAC;EACH,CAAC;EACDC,OAAO,EAAE,CACT,CAAC;EACDC,QAAQ,EAAE;IACRF,IAAI,GAAG;MAAE,OAAO,IAAI,CAACD,MAAM,CAACC,IAAG;IAAE,CAAC;IAClCN,eAAe,GAAG;MAAE,OAAO,IAAI,CAACK,MAAM,CAACL,eAAc;IAAE,CAAC;IACxDS,SAAS,GAAG;MAAE,OAAO,IAAI,CAACJ,MAAM,CAACI,SAAQ;IAAE;EAC7C,CAAC;EAEDC,KAAK,EAAE;IACL,MAAMV,eAAe,CAACW,IAAI,EAAE;MAC1BC,OAAO,CAACC,GAAG,CAAC,MAAM;MAClB,IAAIF,IAAI,EAAE;QACR,IAAIG,KAAI,GAAI,MAAMhB,GAAG,CAACiB,WAAW,CAAC,IAAI,CAACV,MAAM,CAACC,IAAI,CAACU,KAAK;QACxDJ,OAAO,CAACC,GAAG,CAAE,UAASC,KAAM,EAAC;MAC/B;IACF;EACF,CAAC;EACD,MAAMG,OAAO,GAAG;IACdL,OAAO,CAACC,GAAG,CAAC,iBAAiB;IAC7BD,OAAO,CAACC,GAAG,CAACb,eAAe;IAC3B,MAAM,IAAI,C