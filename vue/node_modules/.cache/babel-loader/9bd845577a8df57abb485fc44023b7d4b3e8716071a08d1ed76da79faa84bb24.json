{"ast":null,"code":"import MainNav from \"@/components/layout/MainNav.vue\";\nimport api from \"@/api/switchit\";\nexport default {\n  components: {\n    MainNav\n  },\n  name: \"App\",\n  data() {\n    return {\n      user: this.$auth0.user,\n      isAuthenticated: this.$auth0.isAuthenticated,\n      isLoading: this.$auth0.isLoading\n    };\n  },\n  methods: {},\n  watch: {\n    isAuthenticated(auth) {\n      console.log(`auth: ${auth}`);\n    },\n    async user(user) {\n      if (user) {\n        await api.createToken(user.email, '111');\n      }\n      // console.log(this.$auth.token)\n    }\n  },\n\n  async mounted() {\n    if (this.isAuthenticated) {\n      console.log('authenticated');\n      console.log(`user: ${this.$auth0.user}`);\n      console.log(this.$auth0.user);\n    }\n    // const accessToken = await this.$auth0.getAccessTokenSilently();\n    // console.log(accessToken)\n  }\n};","map":{"version":3,"mappings":"AAYA,OAAOA,OAAM,MAAO,iCAAiC;AACrD,OAAOC,GAAE,MAAO,gBAAgB;AAEhC,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAI,EAAE,KAAK;EACXC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,IAAI,CAACC,MAAM,CAACD,IAAI;MACtBE,eAAe,EAAE,IAAI,CAACD,MAAM,CAACC,eAAe;MAC5CC,SAAS,EAAE,IAAI,CAACF,MAAM,CAACE;IACzB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE,CACT,CAAC;EACDC,KAAK,EAAE;IACLH,eAAe,CAACI,IAAI,EAAE;MACpBC,OAAO,CAACC,GAAG,CAAE,SAAQF,IAAK,EAAC;IAC7B,CAAC;IACD,MAAMN,IAAI,CAACA,IAAI,EAAE;MACf,IAAIA,IAAI,EAAE;QACR,MAAMJ,GAAG,CAACa,WAAW,CAACT,IAAI,CAACU,KAAK,EAAE,KAAK;MACzC;MACA;IACF;EACF,CAAC;;EACD,MAAMC,OAAO,GAAG;IACd,IAAI,IAAI,CAACT,eAAe,EAAE;MACxBK,OAAO,CAACC,GAAG,CAAC,eAAe;MAC3BD,OAAO,CAACC,GAAG,CAAE,SAAQ,IAAI,CAACP,MAAM,CAACD,IAAK,EAAC;MACvCO,OAAO,CAACC,GAAG,CAAC,IAAI,CAACP,MAAM,CAACD,IAAI;IAC9B;IACA;IACA;EACF;AACF,CAAC","names":["MainNav","api","components","name","data","user","$auth0","isAuthenticated","isLoading","methods","watch","auth","console","log","createToken","email","mounted"],"sourceRoot":"","sources":["/Users/nick/Sites/switchit-vue/src/App.vue"],"sourcesContent":["<