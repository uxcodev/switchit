s/ui/FilterGroup.vue';\n\n\nexport default {\n  components: {\n    ModalWindow,\n    FilterGroup\n  },\n  data() {\n    return {\n      modalComponent: null,\n      screen: 'UserTable',\n      // user: this.$store.getters.user\n    }\n  },\n  computed: {\n    user() {\n      return this.$store.getters.user\n    }\n  },\n  methods: {\n    closeModal() {\n      this.modalComponent = null\n    },\n    openModal(component) {\n      this.modalComponent = component\n    },\n    changePage(page) {\n      this.screen = page\n    }\n  },\n  async mounted() {\n    setTimeout(() => {\n      // this.user = \"Hellloooooo\"\n      // this.user = this.$store.getters.user\n    }, 1000)\n  }\n}\n</script>\n\n<style lang=\"sass\" scoped>\n@import \"/src/styles/variables.sass\"\n@import \"/src/styles/mixins.sass\"\n@import \"@vueform/multiselect/themes/default.css\"\n.main\n  display: flex\n  flex-direction: column\n  align-items: center\n  justify-content: flex-start\n  background-color: #fafafa\n  min-height: 100vh\n  padding: 120px 20px 60px 20px\n  top: 80px\nsection\n  display: flex\n  flex-direction: column\n  gap:0px\n.container\n  width:900px\n  display: flex\n  flex-direction: column\n  gap: 20px\n\n  .ph_pills\n    display: flex\n    flex-direction: row\n    gap: 6px\n    div\n      background-color: #eee\n      width: 100px\n      height: 30px\n      border-radius: 20px\n\n  // .ph_boxes\n  //     display: flex\n  //     flex-direction: row\n  //     gap: 16px\n  //     div\n  //       flex:1\n  //       background-color: white\n  //       // width: 100px\n  //       height: 100px\n  //       border-radius: 10px\n  .stats\n      display: flex\n      flex-direction: row\n      gap: 16px\n      .card\n        flex:1\n        display:flex\n        flex-direction: column\n        background-color: white\n        padding: 16px\n        // width: 100px\n        height: 100px\n        border-radius: 10px\n\n        .rating\n          display: flex\n          align-items: center\n          font-size: 20px\n          color: #666\n          gap: 6px\n          .stars\n            display: flex\n            align-items: center\n            color: #00C6C6\n            span\n              width:20px\n              height:20px\n      &-title\n        font-size: 1.2em\n        color:#aaa\n       \n  .ph_boxes_lg\n      display: flex\n      flex-direction: row\n      gap: 16px\n      div\n        flex:1\n        background-color: #eee\n        // width: 100px\n        height: 200px\n        border-radius: 10px\n\n  .ph_header\n    display: flex\n    background-color: #ccc\n    height: 40px\n    width: 100%\n    border-radius: 10px\n\n  .ph_table\n    display: flex\n    background-color: #eee\n    height: 400px\n    width: 100%\n    border-radius: 10px\n\n.ph_boxes\n  display: flex\n  flex-direction: row\n  gap: 6px\n  .ph_box\n    background-color: #eee\n    width: 100px\n    height: 30px\n    border-radius: 20px    \n.checkbox-group\n  flex-direction: row\n  flex-wrap: wrap\n  width:400px\n  label\n    width:160px\npre\n  color: black\n\n// .stars \n//   font-variation-settings: {\n//   'FILL' 1,\n//   'wght' 400,\n//   'GRAD' 0,\n//   'opsz' 48\n//   }\n\n</style>\n\n<style lang=\"scss\">\n.material-symbols-rounded {\n  font-variation-settings: \"FILL\" 1, \"wght\" 400, \"GRAD\" 0, \"opsz\" 48;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}