{"ast":null,"code":"import MainNav from \"@/components/layout/MainNav.vue\";\nimport api from \"@/api/switchit\";\nimport jwt_decode from \"jwt-decode\";\nexport default {\n  components: {\n    MainNav\n  },\n  name: \"App\",\n  data() {\n    return {\n      user: this.$auth0.user\n    };\n  },\n  watch: {\n    async user(user) {\n      if (user) {\n        let token = await api.createToken(user.email);\n        // get user info out of the token and save to VueX\n\n        let decodedToken = jwt_decode(token);\n        await this.$store.dispatch('setUser', decodedToken.user);\n        let switchitUser = await this.$store.getters.user;\n        console.log('user');\n        console.log(switchitUser);\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAYA,OAAOA,OAAM,MAAO,iCAAiC;AACrD,OAAOC,GAAE,MAAO,gBAAgB;AAChC,OAAOC,UAAS,MAAO,YAAY;AAEnC,eAAe;EACbC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAI,EAAE,KAAK;EACXC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,IAAI,CAACC,MAAM,CAACD;IACpB,CAAC;EACH,CAAC;EACDE,KAAK,EAAE;IACL,MAAMF,IAAI,CAACA,IAAI,EAAE;MACf,IAAIA,IAAI,EAAE;QACR,IAAIG,KAAI,GAAI,MAAMR,GAAG,CAACS,WAAW,CAACJ,IAAI,CAACK,KAAK;QAC5C;;QAEA,IAAIC,YAAW,GAAIV,UAAU,CAACO,KAAK;QACnC,MAAM,IAAI,CAACI,MAAM,CAACC,QAAQ,CAAC,SAAS,EAAEF,YAAY,CAACN,IAAI;QAEvD,IAAIS,YAAW,GAAI,MAAM,IAAI,CAACF,MAAM,CAACG,OAAO,CAACV,IAAG;QAChDW,OAAO,CAACC,GAAG,CAAC,MAAM;QAClBD,OAAO,CAACC,GAAG,CAACH,YAAY;MAC1B;IACF;EACF;AACF,CAAC","names":["MainNav","api","jwt_decode","components","name","data","user","$auth0","watch","token","createToken","email","decodedToken","$store","dispatch","switchitUser","getters","console","log"],"sourceRoot":"","sources":["/Users/nick/Sites/switchit-vue/src/App.vue"],"sourcesContent":["<template>\n  <MainNav />\n  <div id=\"app\">\n    <router-view></router-view>\n    <!-- <div class=\"main\">\n      Waiting for authorization...\n    </div> -->\n    <code>{{ user }}</code>\n  </div>\n</template>\n\n<script>\nimport MainNav from \"@/components/layout/MainNav.vue\";\nimport api from \"@/api/switchit\";\nimport jwt_decode from \"jwt-decode\";\n\nexport default {\n  components: {\n    MainNav,\n  },\n  name: \"App\",\n  data() {\n    return {\n      user: this.$auth0.user\n    };\n  },\n  watch: {\n    async user(user) {\n      if (user) {\n        let token = await api.createToken(user.email)\n        // get user info out of the token and save to VueX\n\n        let decodedToken = jwt_decode(token)\n        await this.$store.dispatch('setUser', decodedToken.user)\n\n        let switchitUser = await this.$store.getters.user\n        console.log('user')\n        console.log(switchitUser)\n      }\n    }\n  },\n};\n</script>\n\n<style lang=\"sass\">\n@import \"/src/styles/styles.sass\"\n\n\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}