{"ast":null,"code":"import MainNav from \"@/components/layout/MainNav.vue\";\nimport api from \"@/api/switchit\";\nimport jwt_decode from \"jwt-decode\";\nexport default {\n  components: {\n    MainNav\n  },\n  name: \"App\",\n  data() {\n    return {\n      auth0User: this.$auth0.user\n    };\n  },\n  watch: {\n    async auth0User(auth0User) {\n      if (auth0User) {\n        let token = await api.createToken(auth0User.email);\n        // get user info out of the token and save to VueX\n\n        let decodedToken = jwt_decode(token);\n        await this.$store.dispatch('setUser', decodedToken.user);\n        let switchitUser = await this.$store.getters.user;\n        console.log('user');\n        console.log(switchitUser);\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAYA,OAAOA,OAAM,MAAO,iCAAiC;AACrD,OAAOC,GAAE,MAAO,gBAAgB;AAChC,OAAOC,UAAS,MAAO,YAAY;AAEnC,eAAe;EACbC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,IAAI,EAAE,KAAK;EACXC,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI,CAACC,MAAM,CAACC;IACzB,CAAC;EACH,CAAC;EACDC,KAAK,EAAE;IACL,MAAMH,SAAS,CAACA,SAAS,EAAE;MACzB,IAAIA,SAAS,EAAE;QACb,IAAII,KAAI,GAAI,MAAMT,GAAG,CAACU,WAAW,CAACL,SAAS,CAACM,KAAK;QACjD;;QAEA,IAAIC,YAAW,GAAIX,UAAU,CAACQ,KAAK;QACnC,MAAM,IAAI,CAACI,MAAM,CAACC,QAAQ,CAAC,SAAS,EAAEF,YAAY,CAACL,IAAI;QAEvD,IAAIQ,YAAW,GAAI,MAAM,IAAI,CAACF,MAAM,CAACG,OAAO,CAACT,IAAG;QAChDU,OAAO,CAACC,GAAG,CAAC,MAAM;QAClBD,OAAO,CAACC,GAAG,CAACH,YAAY;MAC1B;IACF;EACF;AACF,CAAC","names":["MainNav","api","jwt_decode","components","name","data","auth0User","$auth0","user","watch","token","createToken","email","decodedToken","$store","dispatch","switchitUser","getters","console","log"],"sourceRoot":"","sources":["/Users/nick/Sites/switchit-vue/src/App.vue"],"sourcesContent":["<template>\n  <MainNav />\n  <div id=\"app\">\n    <router-view></router-view>\n    <!-- <div class=\"main\">\n      Waiting for authorization...\n    </div> -->\n    <code>{{ auth0User }}</code>\n  </div>\n</template>\n\n<script>\nimport MainNav from \"@/components/layout/MainNav.vue\";\nimport api from \"@/api/switchit\";\nimport jwt_decode from \"jwt-decode\";\n\nexport default {\n  components: {\n    MainNav,\n  },\n  name: \"App\",\n  data() {\n    return {\n      auth0User: this.$auth0.user\n    };\n  },\n  watch: {\n    async auth0User(auth0User) {\n      if (auth0User) {\n        let token = await api.createToken(auth0User.email)\n        // get user info out of the token and save to VueX\n\n        let decodedToken = jwt_decode(token)\n        await this.$store.dispatch('setUser', decodedToken.user)\n\n        let switchitUser = await this.$store.getters.user\n        console.log('user')\n        console.log(switchitUser)\n      }\n    }\n  },\n};\n</script>\n\n<style lang=\"sass\">\n@import \"/src/styles/styles.sass\"\n\n\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}