{"ast":null,"code":"import SwitchitIcon from \"@/components/assets/SwitchitIcon.vue\";\nimport LoginButton from \"../auth/LoginButton.vue\";\nimport LogoutButton from \"../auth/LogoutButton.vue\";\nimport api from \"@/api/switchit\";\nexport default {\n  name: \"TheSidebar\",\n  components: {\n    SwitchitIcon,\n    LoginButton,\n    LogoutButton\n  },\n  data() {\n    return {\n      isDisabled: true,\n      isAuthenticated: this.$auth0.isAuthenticated\n    };\n  },\n  computed: {\n    isLoggedIn() {\n      return this.$store.getters.isLoggedIn;\n    },\n    isAdmin() {\n      let user = this.$store.getters.user;\n      if (user.hasOwnProperty('admin')) {\n        return this.$store.getters.user.admin;\n      } else {\n        return false;\n      }\n    }\n  },\n  methods: {\n    gotoSettings() {\n      console.log('clicked');\n    },\n    async createToken() {\n      let token = await api.createToken(\"nto@switchit.ai\");\n      console.log(token);\n    }\n  },\n  mounted() {}\n};","map":{"version":3,"mappings":"AAqBA,OAAOA,YAAW,MAAO,sCAAsC;AAC/D,OAAOC,WAAU,MAAO,yBAAyB;AACjD,OAAOC,YAAW,MAAO,0BAA0B;AACnD,OAAOC,GAAE,MAAO,gBAAgB;AAEhC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IAAEL,YAAY;IAAEC,WAAW;IAAEC;EAAa,CAAC;EACvDI,IAAI,GAAG;IACL,OAAO;MACLC,UAAU,EAAE,IAAI;MAChBC,eAAe,EAAE,IAAI,CAACC,MAAM,CAACD;IAC/B,CAAC;EACH,CAAC;EACDE,QAAQ,EAAE;IACRC,UAAU,GAAG;MACX,OAAO,IAAI,CAACC,MAAM,CAACC,OAAO,CAACF,UAAU;IACvC,CAAC;IACDG,OAAO,GAAG;MACR,IAAIC,IAAG,GAAI,IAAI,CAACH,MAAM,CAACC,OAAO,CAACE,IAAG;MAClC,IAAIA,IAAI,CAACC,cAAc,CAAC,OAAO,CAAC,EAAE;QAChC,OAAO,IAAI,CAACJ,MAAM,CAACC,OAAO,CAACE,IAAI,CAACE,KAAI;MACtC,OAAO;QACL,OAAO,KAAI;MACb;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,YAAY,GAAG;MACbC,OAAO,CAACC,GAAG,CAAC,SAAS;IACvB,CAAC;IACD,MAAMC,WAAW,GAAG;MAClB,IAAIC,KAAI,GAAI,MAAMpB,GAAG,CAACmB,WAAW,CAAC,iBAAiB;MACnDF,OAAO,CAACC,GAAG,CAACE,KAAK;IACnB;EACF,CAAC;EACDC,OAAO,GAAG,CACV;AACF,CAAC","names":["SwitchitIcon","LoginButton","LogoutButton","api","name","components","data","isDisabled","is